<body onLoad="animateDots();">
  <div id="loadingAndTests" class="loading">
      <h2>Whirled is loading<span id="loadingDots">...</span></h2>    
      <noscript><blockquote>
          Whirled requires JavaScript to operate. Please turn it on in your browser.
          <p>If you would like more information, please read our instructions on
          <a href="http://wiki.whirled.com/How_To_Enable_JavaScript">how to turn on JavaScript</a>.
      </blockquote></noscript>
  </div>
  
  <table id="ctable" width="100%" height="100%" cellpadding="0" cellspacing="0"
         style="padding: 0; margin: 0;">
  <tr><td colspan="3" id="header"></td></tr>
  <tr><td valign="top" id="content" style="background: #FFFFFF; display: none"></td>
      <td valign="top" id="client" height="100%"></td></tr>
  </table>
  <iframe id="__gwt_historyFrame" style="width:0px;height:0px;border:0px;display:none"></iframe>
  <script language="javascript" src="/gwt/msoy/msoy.nocache.js"></script>
  <script id="swfobject" type="text/javascript"></script>
  <script id="md5" type="text/javascript"></script>
  <script id="googanal" type="text/javascript"></script>
  @RECAPTCHA_IN@
  <script id="recaptcha" type="text/javascript"></script>
  <script type="text/javascript">
    var recaptchaPublicKey = "@RECAPTCHA_KEY@";
  </script>
  @RECAPTCHA_OUT@
</body>

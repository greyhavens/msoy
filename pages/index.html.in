<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>Whirled</title>
  <meta content="Play games, make friends, explore, chat, shop and create all in your own Whirled."
        name="description"/>
  <meta content="games,friends,explore,chat,shop,create,whirled" name="keywords"/>
  <script type="text/javascript">
    if (document.location.host != "@SERVER_URL_HOST_PORT@") {
       document.location = "@SERVER_URL@";
    }
    
      /** Animate the loading message ellipsis until it is cleared */
      function animateDots() {
        var dots = document.getElementById('loadingDots');
        if (dots == undefined) {
            return;
        }
        
        if (dots.innerHTML == '.') { 
           dots.innerHTML = '..'; 
        }
        else if (dots.innerHTML == '..') { 
           dots.innerHTML = '...'; 
        }
        else{ 
           dots.innerHTML = '.';
        }
        
        setTimeout("animateDots();", 500);
      }
  </script>
  
  <link rel="stylesheet" href="/css/global.css" type="text/css"/>
  <link rel="stylesheet" href="/css/gwt.css" type="text/css"/>
  <link rel="stylesheet" href="/css/frame.css" type="text/css"/>

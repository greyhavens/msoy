 *
 * CSS styles for the Swiftly interfaces.
 */

.swiftlyPanel {
  width: 100%;
  height: 100%;
}

.swiftlyUploader {
  width: 100%;
  background: #DCDCDC;
  border: 1px solid black
}

.projectEdit {
  width: 100%;
  background: #DCDCDC;
  border: 1px solid black
}

.projectEdit .LeftSpacer {
  padding-left: 5px;
}

.projectSelectionPanel {
  width: 100%;
  padding: 10px;
}

.projectSelectionPanel .Header {
  font-size: 14px;
  font-weight: bold;
}

.projectSelectionPanel td {
  vertical-align: top;
}
